{
  "name": "IXORA-fullstack",
  "version": "1.0.0",
  "description": "Aplicación web IXORA - Sistema de Gestión",
  "author": "Pedidos",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node server.js",
    "client": "cd client && npm run start:ip",
    "build": "cd client && npm install && npm run build",
    "server:stop": "cd server && npm run stop",
    "server:restart": "cd server && npm run restart"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "dependencies": {
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "exceljs": "^4.4.0",
    "express": "^5.1.0"
  }
}
